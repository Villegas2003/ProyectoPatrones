<div class="row">
    <div class="col-lg-6">
        <div class="bs-component">
            <form>
            <fieldset>
            <legend>User registration form</legend>

            <div class="form-group">
                <label for="txtIdCity" class="form-label mt-4">Cedula</label>
                <input id="txtId" type="number" class="form-control" aria-describedby="emailHelp" placeholder="Enter Cedula">


                <label for="txtName" class="form-label mt-4">Name</label>
                <input type="text" class="form-control" id="txtName" aria-describedby="emailHelp" placeholder="Enter complete name">
            </div>
            <div class="form-group">
                <label for="txtEmail" class="form-label mt-4">Email</label>
                <input type="text" class="form-control" id="txtEmail" aria-describedby="emailHelp" placeholder="Enter Email">
            </div>
            <div class="form-group">
                <label for="txtPass" class="form-label mt-4">Password</label>
                <input type="password" class="form-control" id="txtPass" aria-describedby="emailHelp" placeholder="Enter Password">
            </div>

            </form>
            <br />
            <button id="btnCreateUser" class="source-button btn btn-primary btn-xs" type="button" tabindex="0"><i class="bi bi-code">Save</i></button>

            <button id="btnUpdateUser" class="source-button btn btn-warning btn-xs" type="button" tabindex="0"><i class="bi bi-code">Update</i></button>

            <button id="btnDeleteUser" class="source-button btn btn-danger btn-xs" type="button" tabindex="0"><i class="bi bi-code">Delete</i></button>
        </div>
    </div>
</div>
</div>